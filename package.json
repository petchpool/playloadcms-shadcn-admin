{
  "name": "playloadcms-shadcn-admin",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev -p 3200",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "seed": "cross-env NODE_OPTIONS=--no-deprecation tsx src/seed/run-seed.ts",
    "seed:languages": "cross-env NODE_OPTIONS=--no-deprecation tsx src/seed/run-seed.ts languages",
    "seed:layouts": "cross-env NODE_OPTIONS=--no-deprecation tsx src/seed/run-seed.ts layouts",
    "seed:pages": "cross-env NODE_OPTIONS=--no-deprecation tsx src/seed/run-seed.ts pages",
    "seed:sites": "cross-env NODE_OPTIONS=--no-deprecation tsx src/seed/run-seed.ts sites",
    "seed:permissions": "cross-env NODE_OPTIONS=--no-deprecation tsx src/seed/run-seed.ts permissions",
    "seed:roles": "cross-env NODE_OPTIONS=--no-deprecation tsx src/seed/run-seed.ts roles",
    "seed:users": "cross-env NODE_OPTIONS=--no-deprecation tsx src/seed/run-seed.ts users",
    "seed:components": "cross-env NODE_OPTIONS=--no-deprecation tsx src/seed/run-seed.ts components",
    "seed:rbac": "cross-env NODE_OPTIONS=--no-deprecation tsx src/seed/run-seed.ts rbac",
    "seed:workflows": "cross-env NODE_OPTIONS=--no-deprecation tsx src/seed/run-seed.ts workflows",
    "seed:navigation": "cross-env NODE_OPTIONS=--no-deprecation tsx src/seed/navigation.ts",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@payloadcms/db-postgres": "3.69.0",
    "@payloadcms/next": "3.69.0",
    "@payloadcms/richtext-lexical": "3.69.0",
    "@payloadcms/ui": "3.69.0",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/postcss": "^4.1.18",
    "@tanstack/react-table": "^8.21.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "dotenv": "16.4.7",
    "framer-motion": "^12.23.26",
    "graphql": "^16.8.1",
    "jotai": "^2.16.0",
    "lucide-react": "^0.562.0",
    "next": "16.1.1",
    "payload": "3.69.0",
    "postcss": "^8.5.6",
    "qs": "^6.14.0",
    "react": "19.2.3",
    "react-day-picker": "^9.13.0",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.69.0",
    "sharp": "0.34.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@playwright/test": "1.56.1",
    "@testing-library/react": "16.3.0",
    "@types/node": "^22.5.4",
    "@types/qs": "^6.14.0",
    "@types/react": "19.2.1",
    "@types/react-dom": "19.2.1",
    "@vitejs/plugin-react": "4.5.2",
    "eslint": "^9.16.0",
    "eslint-config-next": "16.1.1",
    "jsdom": "26.1.0",
    "playwright": "1.56.1",
    "playwright-core": "1.56.1",
    "prettier": "^3.4.2",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  }
}
